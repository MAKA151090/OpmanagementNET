@model HealthCare.Models.IPTypeMasterModel

@{
    ViewData["Title"] = "IPTypeMaster";
    Layout = "~/Views/Shared/_ContentNavbarLayout.cshtml";
}

<div class="card xl-6">
    <div class="card-body pt-2 mt-1">
        <div class="card-header d-flex justify-content-between align-items-center" style="height: 50px;">
            <h2 class="text-primary mb-1"> IPTypeMaster</h2>
            <small class="text-muted float-end"></small>

        </div>
    </div>


        <form asp-action="IPTypeMaster">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="row gy-4">

                    <div class="col-md-12 col-lg-1">
                    </div>
                    <div class="col-md-12 col-lg-4">
                        <div class="card">
                            <div class="card-body">

                                <div class="form-floating form-floating-outline mb-4">
                            <input asp-for="IPTypeID" class="form-control" placeholder="IPTypeID" />
                <label asp-for="IPTypeID" class="control-label"></label>
                
                <span asp-validation-for="IPTypeID" class="text-danger"></span>
            </div>
                        <div class="form-floating form-floating-outline mb-4">
