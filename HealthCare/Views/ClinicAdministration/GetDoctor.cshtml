@model DoctorAdminModel

@{
    ViewData["Title"] = "GetDoctor";
    Layout = "~/Views/Shared/_ContentNavbarLayout.cshtml";
}


<div class="card xl-6">
    <div class="card-body pt-2 mt-1">
        <div class="card-header d-flex justify-content-between align-items-center" style="height: 50px;">
            <h2 class="text-primary mb-1"> EWSMaster</h2>
            <small class="text-muted float-end"></small>

        </div>
        <div class="row"></div>
    </div>
    <div class="row">
        

@using (Html.BeginForm("Doctoradmin", "ClinicAdministration", FormMethod.Post))
{

   
    <div class="row gy-4">

                    <div class="col-md-12 col-lg-1">
                    </div>
                    <div class="col-md-12 col-lg-4">
                        <div class="card">
                            <div class="card-body">

                                <div class="form-floating form-floating-outline mb-4">
                            <input asp-for="DoctorID" class="form-control" placeholder="DoctorID" />
                    <label asp-for="DoctorID" class="control-label"></label>
                    
                    <span asp-validation-for="DoctorID" class="text-danger"></span>
                </div>
                <br />

                <div class="form-group">
                    <button type="submit" name="buttonType" value="submit" class="btn btn-danger">Submit</button>
                </div>
                
        </div>
    </div>
    </div>
    </div>
    

}